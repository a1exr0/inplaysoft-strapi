{
  "name": "my-app-name",
  "version": "0.1.0",
  "private": true,
  "description": "A Strapi application",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 strapi build",
    "deploy": "strapi deploy",
    "deploy:production": "./scripts/deploy-production.sh",
    "develop": "strapi develop",
    "seed:example": "node ./scripts/seed.js",
    "optimize:db": "node ./scripts/optimize-database.js",
    "optimize:db:force": "FORCE_OPTIMIZE=true node ./scripts/optimize-database.js",
    "test:performance": "node ./scripts/test-performance.js",
    "start": "strapi start",
    "start:production": "NODE_ENV=production strapi start",
    "strapi": "strapi"
  },
  "dependencies": {
    "@strapi/plugin-cloud": "5.11.0",
    "@strapi/plugin-seo": "^2.0.8",
    "@strapi/plugin-users-permissions": "5.11.0",
    "@strapi/provider-upload-aws-s3": "^5.15.0",
    "@strapi/strapi": "5.11.0",
    "axios": "^1.7.9",
    "better-sqlite3": "11.3.0",
    "dotenv": "^16.4.7",
    "fs-extra": "^10.0.0",
    "mime-types": "^2.1.27",
    "pg": "^8.16.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0",
    "strapi-cache": "^1.4.9",
    "styled-components": "^6.0.0"
  },
  "engines": {
    "node": ">=20.0.0 <=22.x.x",
    "npm": ">=10.0.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  }
}
